{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "@typescript-eslint/no-explicit-any": 2,
        "@typescript-eslint/explicit-function-return-type": 2,
        "@typescript-eslint/no-unused-vars": 1
    }
}
